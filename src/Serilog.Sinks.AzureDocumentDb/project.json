{
    "title": "Serilog.Sinks.AzureDocumentDB",
    "version": "3.5.17-*",
    "description": "Serilog event sink that writes to Azure DocumentDb",
    "authors": [
        "Serilog Contributors"
    ],
    "packOptions": {
        "tags": [
            "serilog",
            "logging",
            "semantic",
            "structured"
        ],
        "projectUrl": "http://serilog.net",
        "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0",
        "iconUrl": "http://serilog.net/images/serilog-sink-nuget.png"
    },
    "buildOptions": {
        "debugType": "portable",
        "outputName": "Serilog.Sinks.AzureDocumentDB",
        "optimize": true,
        "embed": [ "/**/bulkImport.js" ],
        "define": []
    },
    "dependencies": {
        "Serilog": "2.2.1"
    },
    "frameworks": {
        "netstandard1.6": {
            "dependencies": {
                "Microsoft.Azure.DocumentDB.Core": "0.1.0-*"
            }
        },
        "net452": {
            "dependencies": {
                "Microsoft.Azure.DocumentDB": "1.9.5"
            }
        }
    }
}
